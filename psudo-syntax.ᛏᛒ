include first_saga.tb;

define subtracting(number1,number2){
    return number1 - number2 ;
}

greeting = "heisann";
hei_pointer = *greeting;

liste = [5];
liste[0] = 255;

for(i = 1; i > 10; i ++){
    if(liste[i] != 0){
        subtracting(liste[i],liste[i]);
    }
}

if(subtracting(4,2)>1){
    hei_pointer += 0xF;
}
